material Gazebo/Water
{
  receive_shadows on

  technique
  {
    pass
    {
      ambient 0.5 0.5 0.5 1.000000

      texture_unit
      {
        scale .02 .02
        texture ../textures/water.jpg
        filtering anisotropic
        max_anisotropy 16
      }
    }
  }
}

